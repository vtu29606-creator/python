        "plt.plot(x, df[\"Test2\"], marker='o', label=\"Test2\")\n",
        "plt.plot(x, df[\"Test3\"], marker='*', label=\"Test3\")\n",
        "plt.plot(x, df[\"Test4\"], marker='x', label=\"Test4\")\n",
        "\n",
