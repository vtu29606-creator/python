        "p = 0\n",
        "b = a\n",
        "while b > 0:\n",
        "    r = b % 10\n",
        "    p = p * 10 + r\n",
        "    b = b // 10\n",
        "if p == a:\n",
        "    print(\"Mirror\")\n",
